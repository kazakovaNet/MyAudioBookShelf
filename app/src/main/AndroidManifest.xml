<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ru.kazakova_net.myaudiobookshelf">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.ShelvedActivity"
            android:label="@string/shelved" />
        <activity
            android:name=".activity.CurrentlyListeningActivity"
            android:label="@string/currently_listening" />
        <activity
            android:name=".activity.ArchiveActivity"
            android:label="@string/archive" />
        <activity android:name=".activity.DetailActivity"></activity>
    </application>

</manifest>